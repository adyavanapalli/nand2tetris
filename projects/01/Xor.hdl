/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = a, out = w1);
    Not(in = b, out = w2);

    And(a = a, b = w2, out = w3);
    And(a = w1, b = b, out = w4);

    Or (a = w3, b = w4, out = out);
}